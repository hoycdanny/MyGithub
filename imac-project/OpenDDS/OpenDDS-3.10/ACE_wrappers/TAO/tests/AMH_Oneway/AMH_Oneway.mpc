project(*idl) : taoidldefaults, amh {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*client) : taoserver, amh {
  exename = client
  after += *idl
  source_files {
    client.cpp
    TestS.cpp
    TestC.cpp
  }
  IDL_Files {
  }
}

project(*server) : taoserver, amh, strategies {
  after += *idl
  source_files {
    server.cpp
    TestS.cpp
    TestC.cpp
  }
  IDL_Files {
  }
}
