project(*idl): taoidldefaults {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*Server): rt_server {
  after += *idl
  Source_Files {
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
  IDL_Files {
  }
}

project(*Client): rt_client {
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
  IDL_Files {
  }
}
