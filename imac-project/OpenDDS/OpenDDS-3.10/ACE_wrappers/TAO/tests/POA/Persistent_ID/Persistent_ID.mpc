project(POA_IDL*): taoidldefaults {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(POA_server*): taoserver, avoids_corba_e_micro {
  after += POA_IDL*
  Source_Files {
    testC.cpp
    testS.cpp
    server.cpp
  }
  IDL_Files {
  }
