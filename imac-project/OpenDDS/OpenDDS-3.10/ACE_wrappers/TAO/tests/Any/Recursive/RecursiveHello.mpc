// -*- MPC -*-
//
// $Id: RecursiveHello.mpc 935 2008-12-10 21:47:27Z mitza $

project(*idl): taoidldefaults, valuetype {
  IDL_Files {
    NonRecursive.idl
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, valuetype {
  after += *idl

  Source_Files {
    Hello.cpp
    server.cpp
    TestC.cpp
    TestS.cpp
    NonRecursiveC.cpp
    NonRecursiveS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, valuetype, typecodefactory {
  after += *idl

  Source_Files {
    TestC.cpp
    NonRecursiveC.cpp
    client.cpp
  }
  IDL_Files {
  }
}
