// -*- MPC -*-
// $Id: test.mpc 1861 2011-08-31 16:18:08Z mesnierp $

project(*idl): taoidldefaults {
  idlflags += -Sp
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver, messaging, strategies {
  after += *idl
  Source_Files {
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoserver, messaging, strategies {
  exename = client
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}


