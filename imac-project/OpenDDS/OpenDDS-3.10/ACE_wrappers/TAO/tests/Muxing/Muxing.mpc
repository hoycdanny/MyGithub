project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    Receiver.cpp
    Server_Task.cpp
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, anytypecode {
  after += *idl
  Source_Files {
    Client_Task.cpp
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}
