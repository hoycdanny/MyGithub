// $Id: install_ciao_headers.mpc 2179 2013-05-28 22:16:51Z mesnierp $
project : install {
  custom_only = 1
  Define_Custom(InstallHeader) {
    inputext = .h, _T.cpp
  }
  InstallHeader_Files {
    Version.h
    // The following directories contain header-only code (*.h, *_T.cpp) and
    // are not already installed by other .mpc files:
    Contexts
    Contexts/Session
    Servants/Session
  }
  specific {
    custom_inst_tags = installheader_files
  }
}
