project(*latency_idl): taoidldefaults, strategies {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*latency server): taoserver, strategies {
  after += *latency_idl
  Source_Files {
    Roundtrip.cpp
    TestS.cpp
    TestC.cpp
    server.cpp
  }
  IDL_Files {
  }
}

project(*latency client) : taoclient, strategies {
  after += *latency_idl
  Source_Files {
    TestC.cpp
    Client_Task.cpp
    client.cpp
  }
  IDL_Files {
  }
}

