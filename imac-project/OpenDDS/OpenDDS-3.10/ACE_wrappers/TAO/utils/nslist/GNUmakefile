MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: nsadd nsdel nslist

depend: nsadd-depend nsdel-depend nslist-depend

REMAINING_TARGETS := $(filter-out all depend,$(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nsadd $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nsdel $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nslist $(@)

.PHONY: nsadd
nsadd:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nsadd all

.PHONY: nsadd-depend
nsadd-depend:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nsadd depend

.PHONY: nsdel
nsdel:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nsdel all

.PHONY: nsdel-depend
nsdel-depend:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nsdel depend

.PHONY: nslist
nslist:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nslist all

.PHONY: nslist-depend
nslist-depend:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.nslist depend

project_name_list:
	@echo nsadd
	@echo nsdel
	@echo nslist
