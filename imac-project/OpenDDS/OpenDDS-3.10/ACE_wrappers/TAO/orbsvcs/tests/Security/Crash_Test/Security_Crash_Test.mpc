project(*idl): taoidldefaults {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*server): taoexe, portableserver, ssliop, pi_server, codecfactory, interceptors {
  after += *idl
  exename = server
  Source_Files {
    server.cpp
    test_i.cpp
    testS.cpp
    testC.cpp
  }
  IDL_Files {
  }
}

project(*client): taoexe, portableserver, ssliop, pi_server, codecfactory, interceptors {
  after += *idl
  exename = client
  Source_Files {
    client.cpp
    testC.cpp
  }
  IDL_Files {
  }
}
