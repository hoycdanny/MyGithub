project(*send): avstreamsexe, strategies {
  exename  = sender

  Source_Files {
    Connection_Manager.cpp
    sender.cpp
  }
}

project(*dist): avstreamsexe, strategies {
  exename  = distributer

  Source_Files {
    Connection_Manager.cpp
    distributer.cpp
  }
}

project(*recv): avstreamsexe, strategies {
  exename  = receiver

  Source_Files {
    Connection_Manager.cpp
    receiver.cpp
  }
}

