project(*idl) : taoclient {
  IDL_Files {
    CCS.idl
  }
  custom_only = 1
}

project(*client) : taoclient {
  after += *idl
  source_files {
    client.cpp
    CCSC.cpp
  }
  IDL_Files {
  }
}

project(*server) : taoserver {
  requires += dummy_label
  after += *idl
  source_files {
    server.cpp
    icp.cpp
    CCSC.cpp
    CCSS.cpp
  }
  IDL_Files {
  }
}


