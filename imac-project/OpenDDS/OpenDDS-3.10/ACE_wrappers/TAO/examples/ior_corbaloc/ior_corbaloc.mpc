project(*IDL): taoidldefaults {
  IDL_Files {
    corbaloc.idl
  }
  custom_only = 1
}

project(*server): namingexe, portableserver {
  exename = server

  after += *IDL
  Source_Files {
    corbalocC.cpp
    corbalocS.cpp
    status_i.cpp
    server.cpp
  }
  IDL_Files {
  }
}

project(*client): namingexe {
  exename = client

  after += *IDL
  Source_Files {
    corbalocC.cpp
    ior_corbaloc_client_i.cpp
    client.cpp
  }
  IDL_Files {
  }
}

project(*corbaname client): taoclient {
  exename = corbaname_client

  after += *IDL
  Source_Files {
    corbalocC.cpp
    corbaname_client.cpp
  }
  IDL_Files {
  }
}
