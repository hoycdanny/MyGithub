// $Id: Export.mpc 14 2007-02-01 15:49:12Z mitza $

project(*Lib) : acelib {
  sharedname    = Export_Lib
  dynamicflags += TEST_BUILD_DLL
  Source_Files {
    dll.cpp
  }
}

project(*test) : aceexe {
  exename = test
  after  += *Lib
  libs   += Export_Lib
  Source_Files {
    test.cpp
  }
}
