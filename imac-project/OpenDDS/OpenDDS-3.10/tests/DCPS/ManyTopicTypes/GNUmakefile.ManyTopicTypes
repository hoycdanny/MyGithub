  ifneq ($(TEST_EXE),)
    ## If all of the generated files are there, then we need to check
    ## and make sure that the generated files are up-to-date.  If they are not
    ## then we need the special rule.
    ifeq ($(shell $(TEST_EXE) "Foo3Def.idl" -nt "Foo3DefC.cpp" 2> /dev/null || $(TEST_EXE) "Foo3Def.idl" -nt "Foo3DefC.h" 2> /dev/null || $(TEST_EXE) "Foo3Def.idl" -nt "Foo3DefS.h" 2> /dev/null || $(TEST_EXE) "Foo3Def.idl" -nt "Foo3DefC.inl" 2> /dev/null || echo 0),)
