    ifeq ($(shell $(TEST_EXE) "Fixed.idl" -nt "FixedTypeSupportImpl.cpp" 2> /dev/null || $(TEST_EXE) "Fixed.idl" -nt "Fixed_TS.cpp" 2> /dev/null || $(TEST_EXE) "Fixed.idl" -nt "FixedTypeSupport.idl" 2> /dev/null || $(TEST_EXE) "Fixed.idl" -nt "FixedTypeSupportImpl.h" 2> /dev/null || $(TEST_EXE) "Fixed.idl" -nt "FixedC.h" 2> /dev/null || $(TEST_EXE) "Fixed.idl" -nt "Fixed_TS.hpp" 2> /dev/null || echo 0),)
